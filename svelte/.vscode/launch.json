{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "SvelteKit",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
                "rollup:///src/*": "${webRoot}/*"
            }
        }
    ]
}